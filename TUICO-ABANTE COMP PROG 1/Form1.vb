    Private Sub btnLogin_Click(sender As Object, e As EventArgs) Handles btnLogin.Click
        If txtUser.Text = "admin" And txtPass.Text = "123" Then
            MessageBox.Show("You log in successfully")
            Form3.Show()
            Me.Hide()
        Else
            MessageBox.Show("Wrong Username or Password", "Try again", MessageBoxButtons.OK, MessageBoxIcon.Stop)
            txtUser.Text = ""
            txtPass.Text = ""
        End If
    End Sub
